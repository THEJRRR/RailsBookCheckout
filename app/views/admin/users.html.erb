<h1>Users</h1>
<table class="table">
  <thead>
    <tr>
      <td> email </td>
      <td> is admin </td>
      <td> can review books </td>
    </tr>
  </thead>
<% @users.each do |u| %>
  <tr>
    <td>
      <%= u.email %>
    </td>
    <td>
      <%= render 'make_admin_checkbox', user: u %>
    </td>
    <td>
      <%= render 'make_can_review_checkbox', user: u, temp_val: true %>
    </td>
  </tr>
<% end %>
</table>
